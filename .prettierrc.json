{
    "arrowParens": "avoid",
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-go-template"],
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template"
            }
        }
    ]
}
